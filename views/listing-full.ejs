     <script>
        let form = document.getElementById("filter-form");
        form.addEventListener("submit",function(e){
          e.preventDefault();
          let formdata= new FormData(this);
          fetch("/search",{
            method:"post",
            body:formdata,
  
          }).then(function(response){
            return response.text();
          }).then(function(text){
            let jsonBody=JSON.parse(text);
            for (let i = 0; i < jsonBody.length; i++) {
              const element = jsonBody[i];
              let divs='';
              divs +=`
            
              <div class="wrap-flat-product clearfix">          
                <div class="flat-product">
                  <div class="featured-product">
                    <img src="/images/services/1.jpg" alt="image" />
                    <div class="rate-product">
                      <div class="link-review clearfix">
                        
                          <div class="button-product float-left">
                            <button
                              type="button"
                              class="flat-button"
                              onclick="location.href='#'"
                            >
                           ${element.category} 
                            </button>
                          </div>
                          <div class="start-review float-right">
                            <span class="flat-start">
                              <i class="fa fa-star"></i>
                              <i class="fa fa-star"></i>
                              <i class="fa fa-star"></i>
                              <i class="fa fa-star"></i>
                              <i class="fa fa-star"></i>
                            </span>
                            <a href="#" class="review">( 2 reviewers )</a>
                          </div>
                        </div>
                        <div class="info-product">
                          <h6 class="title">
                            <a href="#">${element.businessName}</a>
                          </h6>
                          <p>${element.address}</p>
                          <a href="#" class="heart">
                            <i class="fa fa-heart-o"></i>
                          </a>
                        </div>
                      </div>
                    </div>
                    <div class="content-product">
                      <div class="tm">${element.name}</div>
                      <div class="text">
                        <p>
                          ${element.description}
                        </p>
                      </div>
                    </div>    
                </div> 
              
            </div>`;
              document.getElementById("searchlist").innerHTML+=divs;
              console.log(divs);
            }
           
          });
        });
     </script>

<script>

  function showFunction() {
    var show = document.getElementById("search-results");
    if(show.style.display === "none"){
      show.style.display = "block";
    }
    else{
      show.style.display = "none";
    }
  }

</script>
</body>
