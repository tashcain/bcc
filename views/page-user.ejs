    <script>
      let details = new XMLHttpRequest();
      details.open("GET", "/myProfile/<%=email%>", true);
      details.send();
      details.onload = function () {
        let jsonBody = JSON.parse(details.response);
        const element = jsonBody[0];
        let info = "";
        info += `
            <h3 class="name">${element.name}</h3>
                        <ul class="info">
                            <li class="phone"><i class="fa fa-phone"></i><a href="#" title="">${element.mobileNumber}</a></li>
                            <li class="email"><i class="fa fa-envelope"></i><a href="#" title="">${element.email}</a></li>
                            <li class="face"><i class="fa fa-facebook-square"></i><a href="#" title="">Facebook</a></li>
                            <li class="twiter"><i class="fa fa-twitter-square"></i><a href="#" title="">Twitter</a></li>
                        </ul>
                        <br>
                        <h3 class="name">My Plan</h3>
                        <p>${element.plan}</p>
            `;
        console.log("get details", details.response);
        document.getElementById("details").innerHTML += info;
        console.log(info);
      };
    </script>
