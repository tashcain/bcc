    <!-- get gallery in footer -->
    <script>
      let gallery = new XMLHttpRequest();
      gallery.open("get", "/getGallery", true);
      gallery.send();
      gallery.onload = function () {
        let galleryJson = JSON.parse(gallery.responseText);
        for (let i in galleryJson) {
          console.log(galleryJson[i]);
          let footerGal = `<li>
                    <div class="thumb">
                      <a href="#" class="effect">
                        <img src="/images/uploaded_images/${galleryJson[i].photo}" alt="image" />
                      </a>
                    </div>
                  </li>`;
          document.getElementById("footerGallery").innerHTML += footerGal;
        }
      };
    </script>
    <script>
      (function () {
        makeRequest();
        function makeRequest() {
          var httpRequest = new XMLHttpRequest(); // Initiatlization of XMLHttpRequest
          if (!httpRequest) {
            alert(" Cannot create an XMLHTTP instance");
            return false;
          }
          httpRequest.onreadystatechange = function () {
            // ready state event, will be executed once the server send back the data
            if (httpRequest.readyState === XMLHttpRequest.DONE) {
              if (httpRequest.status === 200) {
                document.getElementById("username").innerHTML =
                  httpRequest.responseText;
              } else {
                alert("There was a problem with the request.");
              }
            }
          };
          httpRequest.open("GET", "/username"); // service call
          httpRequest.send();
        }
      })();
    </script>
